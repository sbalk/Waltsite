		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.7",
		"svelte-add": "^2024.2.16-0.0",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@tsparticles/slim": "^3.1.0",
		"@tsparticles/svelte": "^3.0.0",
		"tsparticles": "^3.1.0"
	}
}
